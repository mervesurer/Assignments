@Model IEnumarable<Category> 

    <div class="list-group">
        <a href="#" class="list-group-item list-group-item-action">Anasayfa</a>
        @foreach(var category in Model)
        {
            <a asp-action="Index" asp-controller="Home" asp-route-catid="@category.Id" 
               class="list-group-item list-group-item-action">@category.Name</a>
        }     
    </div>